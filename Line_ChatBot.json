        "jsCode": "const event = $node[\"Webhook Trigger\"].json.body?.events?.[0];\nif (!event) {\n  return [{ json: { error: \"Missing LINE event payload\" } }];\n}\nconst userId = event.source?.userId || \"unknown-user\";\nconst replyToken = event.replyToken || \"\";\nconst msgType = event.message?.type;\nlet msg = \"\";\nlet mediaId = \"\";\n\nif (msgType === \"text\") {\n  msg = event.message.text || \"\";\n} else if (msgType === \"image\" || msgType === \"file\") {\n  mediaId = event.message.id || \"\";\n}\n\nreturn [{\n  json: {\n    userId,\n    replyToken,\n    type: msgType,\n    message: msg,    \n    mediaId          // ÂúñÁâá/Ê™îÊ°àÊâçÊúâÂÄº\n  }\n}];\n"
